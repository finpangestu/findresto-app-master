{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "operator-linebreak": "off",
        "func-names":"off",
        "no-restricted-globals" :"off",
        "implicit-arrow-linebreak":"off",
        "indent":"off",
        "import/no-extraneous-dependencies":"off",
        "no-extra-semi":"off" 
    }
}
