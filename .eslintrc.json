{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "operator-linebreak": "off",
        "no-restricted-globals" :"off",
        "implicit-arrow-linebreak":"off",
        "consistent-return": "off",
        "no-prototype-builtins": "off",
        "no-undef": "off"
    }
}
